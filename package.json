    "test:all": "npm run lint && npm run test",
    "eject": "react-scripts eject",
    "ionic": "ionic",
    "cy": "cypress open",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:all"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
